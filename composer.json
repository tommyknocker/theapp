    "autoload": {
        "files": [
            "data/shortcuts.php"
        ],
        "psr-4": {
            "App\\Abstracts\\": "abstracts",
            "": "classes",
            "App\\": "classes",
            "App\\Core\\": "core",
            "App\\Exceptions\\": "exceptions",
            "App\\Interfaces\\": "interfaces",
            "App\\Models\\": "models",
            "App\\Traits\\": "traits"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "monolog/monolog": "^1.17",
        "sabre/event": "^2.0",
        "kriswallsmith/assetic": "^1.3",
        "lmammino/jsmin4assetic": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "phpunit/phpunit-skeleton-generator": "^2.0"
    }
